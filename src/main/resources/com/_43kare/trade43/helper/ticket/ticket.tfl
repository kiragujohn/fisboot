<html>
    <body>
        <table width="100%" border="1" cellspacing="5" cellpadding="5">
            <tbody>
                <tr>
                    <td colspan="2">
                        ${eventticket_ticketTitle}
                    </td>
                </tr>
                <tr>
                    <td align="left" width="170px">
                        <img src="${eventticket_valueQrCodeImagePath}"/> 
                    </td>
                    <td align="left" valign="top">
                        <table cellspacing="5" cellpadding="5">
                            <tbody>
                                <tr>
                                    <td>${eventticket_labelEventName}</td> 
                                    <td>${eventticket_valueEventName}</td>
                                </tr>
                                <tr>
                                    <td>${eventticket_labelMemberName}</td>
                                    <td>${eventticket_valueMemberName}</td>
                                </tr>
                                <tr>
                                    <td>${eventticket_labelEventStart}</td>
                                    <td>${eventticket_valueEventStart}</td>
                                </tr>
                                <tr>
                                    <td>${eventticket_labelEventEnd}</td>
                                    <td>${eventticket_valueEventEnd}</td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <table cellspacing="5" cellpadding="5">
                            <tbody>
                                <tr>
                                    <td width="130px">${eventticket_labelTicketType}</td>
                                    <td>${eventticket_valueTicketType}</td>
                                </tr>
                                <tr>
                                    <td>${eventticket_labelTicketCost}</td>
                                    <td>${eventticket_valueTicketCost}</td>
                                </tr>
                                <tr>
                                    <td>${eventticket_labelTicketNumber}</td>
                                    <td>${eventticket_valueTicketNumber}</td>
                                </tr>
                                <tr>
                                    <td>${eventticket_labelVenue}</td>
                                    <td>${eventticket_valueVenue}</td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                </tr>
         
                    <tr>
                        <td colspan="2">
                            <table cellspacing="5" cellpadding="5">
                                <tbody>
                                    <tr>
                                        <td>${eventticket_labelContact}</td>
                                        <td>
                                            <table cellspacing="5" cellpadding="5">
                                                <tbody>
                                                    <#list eventticket_eventStaffs as eventStaff>
                                                        <tr>
                                                            <td>${eventStaff}</td>
                                                        </tr>
                                                    </#list>
                                                </tbody>
                                            </table>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </td>
                    </tr>
               
                <tr>
                    <td colspan="2">
                        <table cellspacing="5" cellpadding="5">
                            <tbody>
                                <tr>
                                    <td colspan="2">${eventticket_labelEventDescription}</td>
                                </tr>
                                <tr>
                                    <td colspan="2">${eventticket_valueEventDescription}</td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                </tr>
            </tbody>
        </table>
    </body>
</html>